<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="006_add_latte_to_product" author="Tamara Murazyan">
        <insert tableName="product">
            <column name="name" value="Latte"/>
            <column name="description" value="A milk-based coffee drink, which is a three-layer mixture of milk foam."/>
            <column name="price" value="12.00"/>
            <column name="quantity" value="35"/>
            <column name="category_id" value="1"/>
            <column name="image_name" value="latte.jpg"/>
        </insert>
    </changeSet>

</databaseChangeLog>
