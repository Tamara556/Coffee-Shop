<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="008_add_mocha_to_product" author="Tamara Murazyan">
        <insert tableName="product">
            <column name="name" value="Mocha"/>
            <column name="description" value="Type of cappuccino with the addition of white, dark, or milk chocolate."/>
            <column name="price" value="10.00"/>
            <column name="quantity" value="12"/>
            <column name="category_id" value="1"/>
            <column name="image_name" value="mocha.jpg"/>
        </insert>
    </changeSet>

</databaseChangeLog>
