<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="007_add_caramel_macchiato_to_product" author="Tamara Murazyan">
        <insert tableName="product">
            <column name="name" value="Caramel Macchiato"/>
            <column name="description" value="Espresso is added to milk, not the other way around."/>
            <column name="price" value="15.00"/>
            <column name="quantity" value="5"/>
            <column name="category_id" value="1"/>
            <column name="image_name" value="caramel_macchiato.jpg"/>
        </insert>
    </changeSet>

</databaseChangeLog>
